{
  "name": "serialconnectioncapacitor",
  "version": "0.0.6",
  "description": "for serial connection",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Sources",
    "ios/Tests",
    "Package.swift",
    "Serialconnectioncapacitor.podspec"
  ],
  "author": "touyara",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laoapps/Serial_Connection_Capacitor.git"
  },
  "bugs": {
    "url": "https://github.com/laoapps/Serial_Connection_Capacitor/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "xcodebuild -scheme Serialconnectioncapacitor -destination generic/platform=iOS",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --fix --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\" --plugin=prettier-plugin-java",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api SerialPortPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.mjs",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "5.0.5",
    "@capacitor/core": "5.1.1",
    "@capacitor/docgen": "^0.2.0",
    "@capacitor/ios": "5.0.5",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^2.0.0",
    "@ionic/swiftlint-config": "^1.1.2",
    "eslint": "^7.32.0",
    "prettier": "^2.3.2",
    "prettier-plugin-java": "^1.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "typescript": "~4.1.5"
  },
  "peerDependencies": {
    "@capacitor/core": "5.0.5"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "dependencies": {
    "@capacitor/cli": "^7.0.1"
  }
}
