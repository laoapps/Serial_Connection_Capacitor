{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "declaration": true,
    "esModuleInterop": true,
    "inlineSources": true,
    "lib": ["dom", "es5","ES2015"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist/esm",
    "pretty": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES5"
  },
  "files": ["src/android.ts"]
}
